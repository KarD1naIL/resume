<div>
    {{#with main}}
    <main>
        <section>
            {{> title data="My Educationss" big=true}}
            <ul class="ps-5">
                {{#each name}}
                <li>
                    {{name}}
                    {{#unless isEnd}} (ще в процесі) {{/unless}}

                </li>
                {{/each}}
            </ul>
        </section>
        <section>
            {{> title data="My Certification" big=false}}
            <ul class="ps-5">
                {{#each certificates}}
                <li>
                    {{name}} ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>